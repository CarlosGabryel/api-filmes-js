{
  "name": "api-filmes-js",
  "version": "1.0.0",
  "description": "API REST de Filmes com Node.js e Express",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest --coverage",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^5.1.0"
  }, 
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "app.js"
    ],
    "coverageThreshold": {
      "./app.js": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "global": {
        "lines": 80
      }
    }
  }
}